# PyMOL script generated for sector visualization
# Delete any existing sector selections first
delete sector_*

# Create and color sector selections
select sector_0_part0, resi 
select sector_0_part1, resi 
select sector_0_part2, resi 
select sector_0_part3, resi 1057+1094+1095+1135+1144+1154+1160+1167+1180+1225+1228+1231
select sector_0, sector_0_part0 or sector_0_part1 or sector_0_part2 or sector_0_part3
delete sector_0_part0
delete sector_0_part1
delete sector_0_part2
delete sector_0_part3
color yellow, sector_0
show sticks, sector_0

select sector_3_part0, resi 
select sector_3, sector_3_part0
delete sector_3_part0
color purple, sector_3
show sticks, sector_3

select sector_1_part0, resi 204+294+308+317+320+362+366+384+466+471+539+541+594+620+625+628+640+644+647+651+652+657+680+681+689+699+703+709+715+723+726+730+776+832+834+844+849+874+886+895+927+930+956+1044
select sector_1, sector_1_part0
delete sector_1_part0
color red, sector_1
show sticks, sector_1

select sector_2_part0, resi 304+622+658+690
select sector_2, sector_2_part0
delete sector_2_part0
color blue, sector_2
show sticks, sector_2

# Visualization settings
set stick_radius, 0.2
set sphere_scale, 0.3
bg_color white
set ray_opaque_background, off
